{
  "name": "@towbe/serverless-apollo-template",
  "version": "1.0.0",
  "description": "A template for an Apollo Serverless project on AWS",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development npm-run-all --parallel build:dev start:server",
    "start:server": "cd build && NODE_ENV=development nodemon --delay 0.9 ./main",
    "build:dev": "NODE_ENV=development webpack --config webpack.dev.js --watch",
    "deploy": "NODE_ENV=production serverless deploy",
    "lint": "eslint .",
    "clean": "node ./scripts/prepare",
    "update-template": "git fetch template-repo && git merge template-repo/master"
  },
  "keywords": [
    "serverless",
    "apollo",
    "graphql"
  ],
  "dependencies": {
    "@apollo/federation": "^0.11.2",
    "@babel/runtime": "^7.7.7",
    "@firebase/app": "^0.4.17",
    "@firebase/auth": "^0.12.0",
    "@towbe/js-tools": "^1.0.31",
    "apollo-server": "^2.9.3",
    "apollo-server-errors": "^2.3.3",
    "apollo-server-lambda": "^2.9.3",
    "aws-sdk": "^2.521.0",
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "dynamodb-marshaler": "^2.0.0",
    "firebase-admin": "^8.5.0",
    "graphql": "^14.5.4",
    "graphql-import": "^0.7.1",
    "graphql-transform-schema": "^0.2.2",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "serverless-plugin-ifelse": "^1.0.5",
    "serverless-pseudo-parameters": "^2.5.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "serverless": "^1.60.4",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-plugin-include-dependencies": "^3.2.1",
    "serverless-python-requirements": "^5.0.0",
    "serverless-webpack": "^5.3.1",
    "start-server-webpack-plugin": "^2.2.5",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.10.1",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "LinkIt",
  "license": "UNLICENSED"
}
